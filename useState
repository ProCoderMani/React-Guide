import React, { useState } from 'react'

const App = () => {
  // define value and function 
 const[name, setName] =  useState('Hi Mansih');

 const changeHandler =()=>{
  setName("Welcome here on my Repo Let's Learn")
 }

  return (
    <React.Fragment>
    <h1>{name}</h1>
    <button onClick={changeHandler}>Click me</button>

    </React.Fragment>
  )
}

export default App
